{
  "name": "mbapp-api",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "engines": { "node": ">=20" },
  "scripts": {
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist/index.js",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true}); require('fs').rmSync('dist.zip',{force:true})\""
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@aws-sdk/lib-dynamodb": "^3.637.0"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "@types/node": "^20.12.7",
    "esbuild": "^0.23.0"
  }
}
